<%= title 'Sign Up - Application Submitted' %>
<% if current_user.approved? %>
<section class="box-section">
  <div class="row expanded floating-box finish">
    <div class="columns text-center">
      <!-- Verified Account icon by Icons8 -->
      <img class="icon icons8-Verified-Account" width="128" height="128" src="data:image/png;base64,">
      <h1>Application Approved.</h1>
      <h3>Congratulations! Your account has been verified.</h3>

      <%= link_to 'ACCESS DEALS', wizards_path(step: 6), method: :patch, class: 'button' %>
    </div>
  </div>
</section>
<% else %>
<section class="box-section">
  <div class="row expanded floating-box finish">
    <div class="columns text-center">
      <!-- In Progress icon by Icons8 -->
      <img class="icon icons8-In-Progress" width="96" height="96" src="data:image/png;base64,">
      <h1>Application Completed.</h1>
      <h3>Your application has been received! We will be in touch with you very soon. </h3>

      <button data-open="invite-friends" type="button" class="button">Invite Others to Join</button>
    </div>
  </div>
</section>
<% end %>
<div class="reveal" id="invite-friends" data-reveal data-close-on-click="true" data-animation-in="fade-in" data-animation-out="fade-out">
  <div id="step-one">
    <h3>Invite Friends</h3>
    <div class="row">
      <form>
        <div id="invite">
          <div class="row">
            <div class="small-6 columns">
              <label>Email Address
                <input type="text">
              </label>
            </div>
            <div class="small-6 columns">
              <label>Phone Number
                <input type="text">
              </label>
            </div>
          </div>
        </div>
      </form>
    </div>
    <div class="row">
      <div class="columns add">
        <button type="button">Add New Friend</button>
      </div>
    </div>
    <button type="button" class="button next float-right">SEND</button>

    <button class="close-button" data-close aria-label="Close reveal" type="button">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  <div id="step-two" class="text-center">
    <div class="check-circle">
      <span class="icon-big-check"></span>
    </div>        
    <h3>Invitation Sent!</h3>
    <p>We have sent your invitations to your colleagues</p>
    <button data-close="" type="button" class="button">CLOSE</button>
  </div>
</div>