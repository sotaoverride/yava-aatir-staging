<div class="reveal v" id="active_deal" data-reveal>
  <h1>Are you sure you want to activate?</h1>
  <div class="text_center">
    <button class="button order_btns success" data-open="cancel_yes">YES</button>
    <button class="button order_btns alert" data-close aria-label="Close reveal">NO</button>
  </div>

  <button class="close-button" data-close aria-label="Close reveal" type="button">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<!-- This is the nested modal -->
<div class="reveal text_center" id="cancel_yes" data-reveal>
  <p>Order Actived</p>
  <button class="button order_btns success">OK</button>
  <button class="close-button" data-close aria-label="Close reveal" type="button">
    <span aria-hidden="true">&times;</span>
  </button>
</div>

